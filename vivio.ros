#!/bin/sh
#|-*- mode:lisp -*-|#
#| <Put a one-line description here>
exec ros -Q -- $0 "$@"
|#
(load (make-pathname :defaults *load-pathname* :name "vivio" :type "asd"))
(ql:quickload :vivio)

(defun main (&rest argv)
  (declare (ignorable argv))
  (unwind-protect
       (vivio-cui:init-screen)
    (vivio-cui:end-screen)))
